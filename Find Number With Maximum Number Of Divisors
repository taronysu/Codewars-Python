https://www.codewars.com/kata/588c0a38b7cd14085300003f/train/python

def div_num(a, b):
    if a > b:
        return "Error"
    
    def count_divisors(n):
        count = 0
        for i in range(1, int(n**0.5) + 1):
            if n % i == 0:
                count += 1
                if i != n // i:
                    count += 1
        return count

    max_divisors = 0
    result = a

    for num in range(a, b + 1):
        divisors = count_divisors(num)
        if divisors > max_divisors:
            max_divisors = divisors
            result = num
        elif divisors == max_divisors and num < result:
            result = num

    return result
