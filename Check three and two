https://www.codewars.com/kata/5a9e86705ee396d6be000091/train/python

def check_three_and_two(array):
    a = 0
    b = 0
    c = 0
    for i in range(len(array)):
        if array[i] == 'a':
            a += 1
        if array[i] == 'b':
            b += 1
        if array[i] == 'c':
            c += 1
    if (a == 3 and b == 2) or (a == 3 and c == 2) or (b == 3 and a == 2) or (b == 3 and c == 2) or (c == 3 and a == 2) or (c == 3 and b == 2):
        return True
    return False
