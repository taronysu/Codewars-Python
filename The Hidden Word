def hidden(num):
    k = {'6': 'a', '1': 'b', '7': 'd', '4': 'e', '3': 'i', '2': 'l', '9': 'm', '8': 'n', '0': 'o', '5': 't'}
    return ''.join(k[e] for e in str(num))    
def hidden(num):
    return str(num).translate(str.maketrans("6174329805", "abdeilmnot"))
