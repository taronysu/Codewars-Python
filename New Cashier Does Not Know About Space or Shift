def get_order(order):
    menu = ["Burger", "Fries", "Chicken", "Pizza", "Sandwich", "Onionrings", "Milkshake", "Coke"]
    res = ""
    for i in menu:
        n = order.count(i.lower())
        res += (i + " ") * n
    return res[:-1]
