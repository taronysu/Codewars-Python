def leo(oscar):
    if oscar == 88:
        return "Leo finally won the oscar! Leo is happy"
    elif oscar==86:
        return "Not even for Wolf of wallstreet?!"
    elif oscar < 88 and oscar!= 86:
        return "When will you give Leo an Oscar?"
    return "Leo got one already!"
