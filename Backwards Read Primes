https://www.codewars.com/kata/5539fecef69c483c5a000015/train/python

def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

def reverse_number(n):
    return int(str(n)[::-1])

def backwards_prime(start, end):
    backwards_primes = []
    for num in range(start, end + 1):
        if is_prime(num):
            reversed_num = reverse_number(num)
            if reversed_num != num and is_prime(reversed_num):
                backwards_primes.append(num)
    return sorted(backwards_primes)
